[Unit]
Description=uWSGI instance to serve with Flask + Nginx in this projectName
After=network.target

[Service]
User=userName
Group=www-data
Restart=always
WorkingDirectory=pathProject
Environment="PATH=pathProject/venv/bin"
ExecStart=pathProject/venv/bin/uwsgi --ini server_config/project.ini

[Install]
WantedBy=multi-user.target
